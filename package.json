{
  "name": "mdvs_webpack_boilerpate",
  "version": "1.2.0",
  "private": true,
  "description": "Simple Webpack Boilerplate",
  "scripts": {
    "build": "rm -rf dist && webpack --mode production && git add dist",
    "lint-fix": "stylelint 'src/**/*.scss' --fix && eslint src --fix",
    "postcommit": "git update-index --again",
    "precommit": "lint-staged",
    "start": "webpack-dev-server --mode development",
    "dev": "webpack-dev-server --mode development"
  },
  "lint-staged": {
    "ignore": [
      "**/dist/**/*",
      "**/icomoon/**/*"
    ],
    "linters": {
      "*.scss": [
        "stylelint --fix",
        "git add"
      ],
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.6",
    "click-outside": "^2.0.2",
    "foundation-sites": "^6.7.5",
    "gsap": "^3.5.1",
    "jquery": "^3.7.0",
    "letteringjs": "^0.7.0",
    "line-height": "^0.3.1",
    "lodash": "^4.17.11",
    "node-contains": "^1.0.0",
    "odometer": "^0.4.8",
    "scrollmagic": "^2.0.8",
    "sharp": "0.29.0",
    "swiper": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^4.3.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "file-loader": "^6.1.0",
    "fontface-loader": "^0.0.8",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^4.3.0",
    "imports-loader": "^1.1.0",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss-loader": "^4.3.0",
    "pug": "^3.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.49.9",
    "sass-loader": "^10",
    "sass-resources-loader": "^2.2.5",
    "simple-pug-loader": "^0.2.1",
    "sort-css-media-queries": "^1.5.0",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-use-variable": "^1.7.3",
    "stylelint-scss": "^3.21.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-glsl-loader": "^1.0.1"
  },
  "engines": {
    "node": ">=14.21"
  }
}
